[pull]
    rebase = merges
[push]
    default = current
[rebase]
    autostash = true
[advice]
    statusHints = false
[color]
    ui = true
[completion]
    commands = -gitk
[fetch]
    prune = true
[ui]
    editor = vim
[url "git@github.com:"]
    insteadOf = "gh:"
    pushInsteadOf = "github:"
    pushInsteadOf = "https://github.com/"
    pushInsteadOf = "git://github.com/"
